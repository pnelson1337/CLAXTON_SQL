    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataRepo1</DataSourceName>
        <CommandText>DECLARE @TODAY DATE
SET @TODAY = GETDATE()


SELECT

 SUM(CASE WHEN BV.InpatientOrOutpatient = 'I' THEN BCT.Amount END) AS 'Inpatient Charges'
,SUM(CASE WHEN BV.InpatientOrOutpatient = 'O' THEN BCT.Amount END) AS 'Outpatient Charges'
,SUM(BCT.Amount)												   AS 'Total Charges'	

FROM [Livedb].[dbo].[BarChargeTransactions]													AS BCT
INNER JOIN [Livedb].[dbo].[BarVisits] BV ON BV.VisitID=BCT.VisitID
WHERE BatchDateTime &gt;= DATEADD(DAY,-1, CAST(GETDATE() AS DATE)) AND BatchDateTime &lt; CAST(CAST(GETDATE() AS DATE) AS DATETIME)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Inpatient_Charges">
          <DataField>Inpatient Charges</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Outpatient_Charges">
          <DataField>Outpatient Charges</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Charges">
          <DataField>Total Charges</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
