  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT

 PPEmp.Name																		AS 'Name'			
,PPEmp.UniquePublicIdentifier													AS 'SS#'
,UPPER(PPEmp.EmailAddress)														AS 'Email'

FROM [Livedb].[dbo].[PpEmployees] PPEmp
JOIN [Livedb].[dbo].[PpEmployeePayroll] PPEPay ON PPEPay.EmployeeID=PPEmp.EmployeeID

WHERE PPEmp.EmailAddress IS NOT NULL

AND PPEPay.Status IN ('ACTIVE')

ORDER BY Email</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SS_">
          <DataField>SS#</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Email">
          <DataField>Email</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
